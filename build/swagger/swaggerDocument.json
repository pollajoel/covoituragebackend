{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "Covoiturage Api Doc",
        "description": "Yet Another Node.js Blogg Application API",
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        }
    },
    "host": "covoituragebackend.herokuapp.com",
    "basePath": "/api/v1",
    "tags": [
        {
            "name": "Users",
            "description": "API for users in the system"
        }
    ],
    "schemes": [
        "https"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/users": {
            "post":{
                "tags":["Users"],
                "description": "Create new user in system",
                "summary": "Add new user in system",
                "parameters": [{
                    "name": "user",
                    "in": "body",
                    "description": "User that we want to create"
                }],
                "responses": {
                    "200": {"description":"OK"}
                }
            } ,

            "get": {
                "tags": ["Users"],
                "summary": "Get all users in system",
                "responses": {
                    "200": {"description": "OK"},
                    "400": {"description": "OK"}
                }
            }

        },
        "/users/{id}": {
            "parameters": [
              {
                "name": "id",
                "in": "path",
                "required": true,
                "description": "ID of user that we want to find",
                "type": "string"
              }
            ],
            "get": {
              "tags": [
                "Users"
              ],
              "summary": "Get user with given ID",
              "responses": {
                "200": {
                  "description": "user is found"
                }
              }
            }
            },




        "/trips": {
            "post": {
                "tags": ["trips"],
                "description": "Create new trip in the system",
                "summary": "Add new trip in system",
                "parameters": [{
                    "name": "trip",
                    "in": "body",
                    "description": "trap that we want to create"
                }],
                "responses": {
                    "200": {"description":"OK"}
                }
            },
            "get": {
                "tags": ["trips"],
                "summary": "Get all Trips in system",
                "responses": {
                    "200": {"description": "OK"}
                }
            }
        },
        "/trips/{id}": {
            "parameters": [
              {
                "name": "id",
                "in": "path",
                "required": true,
                "description": "ID of trip that we want to find",
                "type": "string"
              }
            ],
            "get": {
              "tags": [
                "trips"
              ],
              "summary": "Get trip with given ID",
              "responses": {
                "200": {
                  "description": "trips is found"
                }
              }
            }
            },


        "/cars": {
            "post": {
                "tags": ["cars"],
                "description": "Create new user Car"
            },
            "get": {
                "tags": ["Car"],
                "summary": "Get user Car",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/upload": {
            "post":{
                "tags":["upload"],
                "description": "Add new image in system",
                "summary": "Add new image in system",
                "parameters": [{
                    "name": "file",
                    "in": "formData",
                    "type": "file",
                    "description": "image that we want to create"
                }],
                "responses": {
                    "200": {"description":"OK"}
                }
            } ,
            "get": {
                "tags": ["upload"],
                "summary": "Get all image",
                "responses": {
                  "200": {
                    "description": "images is found"
                  }
                }
              }
        
        }





    }
}