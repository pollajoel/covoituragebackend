{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "Covoiturage Api Doc",
        "description": "Yet Another Node.js Blogg Application API",
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        }
    },
    "host": "covoituragebackend.herokuapp.com",
    "basePath": "/api/v1",
    "tags": [
        {
            "name": "Users",
            "description": "API for users in the system"
        }
    ],
    "schemes": [
        "https"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/users": {
            "post":{
                "tags":["Users"],
                "description": "Create new user in system",
                "summary": "Add new user in system",
                "parameters": [{
                    "name": "user",
                    "in": "body",
                    "description": "User that we want to create"
                }],
                "responses": {
                    "200": {"description":"OK"}
                }
            } ,

            "get": {
                "tags": ["Users"],
                "summary": "Get all users in system",
                "responses": {
                    "200": {"description": "OK"},
                    "400": {"description": "OK"}
                }
            }

        },
        "/trips": {
            "post": {
                "tags": ["Trip"],
                "description": "Create new trip in the system",
                "summary": "Add new trip in system",
                "parameters": [{
                    "name": "user",
                    "in": "body",
                    "description": "User that we want to create"
                }]
            },
            "get": {
                "tags": ["trips"],
                "summary": "Get all Tri in system",
                "responses": {
                    "200": {"description": "OK"}
                }
            }
        },

        "/cars": {
            "post": {
                "tags": ["cars"],
                "description": "Create new user Car"
            },
            "get": {
                "tags": ["Car"],
                "summary": "Get user Car",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }




    }
}